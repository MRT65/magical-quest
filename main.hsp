*prog_start
	onerror goto *prog_error
	
	randomize
*prog_varreset
	mes "初期化中..."
	common_myhp = 0
	common_myexp = 0
	common_mylevel = 0
	battle_attackamount = 0
	enemy_hp = 0
	battle_remainexp = 0
