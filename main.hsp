*prog_dim
	dim item_have
	sdim enemies_name
	dim enemiesHP
	dim enemiesimg
	dim enemieshpr
	dim enemiesAPr
	dim enemiesAP
	dim enemiesExp
	dim enemiesExpr
	
	log "配列初期化完了" + ptime
	
	wait 30
	mes "初期化終了..."
	wait 20
*game_title
	cls
	mes "マジカルクエスト"
	button "はじめから",*game_start_nodate
	button "つづきから",*game_start_continue
	button "終わる",*game_end
	stop
*game_end
	end
*game_start_nodate
	cls
	mes "セーブデータ作成中..."
	game_savedatep = "def:none"
	common_myhp = 100
	wait 30
	goto *game_reset

*game_start_continue
	cls
	dialog "svm",16,"マジカルクエストセーブファイル"
	wtext = ""
	notesel wtext
	game_savedatep = refstr
	noteload game_savedatep
	noteget i,0
	if i != 1 {
		prog_debug1 = "セーブデータのバージョンが異なります。"
		prog_debug2 = "セーブデータバージョン変換ツールを使用してバージョンを1にしてください"
		prog_errc = "E160"
		goto *prog_error
	}
	noteget common_myhp,0
	noteget commoon_myexp,1
	noteget item_have(0),2
	common_mylevel = common_myexp/100
	goto *game_reset

*game_reset
	wait 15
	
	mes "敵の情報を読み込んでいます..."
	enemydate = ""
	notesel enemydate
	noteload "enemy.txt"
	wait 15
	mes "敵の情報を解析しています..."
	i = 0		;現在の行
	j = 0		;現在の敵ID
*game_date_analysis
	repeat
		k = 0					;読み込みフラグ
		noteget wtext2,i
		if i >= noteinfo(0) {
			break
		}
		if wtext2 == "start" {
			if game_load_status = 0 {
				game_load_status = 1
				i ++
				j ++
			}else{
				prog_debug1 = "敵の情報ファイルが破損しています。"
				prog_debug2 = "エラー番号は@E100です。製作者に報告してください。"
				prog_errc = "E100"
				goto *prog_error
			}
			k = 1
		}
		if wtext2 == "End" {
			if game_load_status = 0 {
				prog_debug1 = "敵の情報ファイルが破損しています。"
				prog_debug2 = "エラー番号は@E150です。製作者に報告してください。"
				prog_errc = "E150"
				goto *prog_error
			}else{
				game_load_status = 0
				i ++
				k = 1
			}
		}
		if wtext2 == "comment" {
			i += 2
			k = 1
		}
		if wtext2 == "Name" {
			noteget wtext,i+1
			if game_load_status = 1 {
				enemies_name(j) = wtext
				i += 2
				k = 1
			}else{
				prog_debug1 = "" + i + "行目の、敵ID" + j+ "の敵の情報ファイルが破損しています。"
				prog_debug2 = "エラー番号は@E101です。製作者に報告してください。"
				prog_errc = "E101"
				goto *prog_error
			}
			
		}
		if wtext2 == "ImagePos" {
			noteget wtext,i+1
			if game_load_status = 1 {
				enemiesimg(j) = int(wtext)
				i += 2
				k = 1
			}else{
				prog_debug1 = "" + i + "行目の、敵ID" + j+ "の敵の情報ファイルが破損しています。"
				prog_debug2 = "エラー番号は@E102です。製作者に報告してください。"
				prog_errc = "E102"
